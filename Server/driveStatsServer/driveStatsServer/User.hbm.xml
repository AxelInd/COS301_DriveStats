<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="driveStatsServer"
                   namespace="driveStatsServer">
  <class name="User" lazy="false" table="users">
    <id name="ID">
      <generator class ="identity"/>
    </id>
    <property name="email" />
    <property name="joinDate" />
    <property name ="averageScore"/>
  </class>

</hibernate-mapping>
